  "main": "dist/index",
  "typings": "dist/index",
  "files": [
    "dist",
    "src"
  ],
  "module": "dist/sdk.esm.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "export-abis": "yarn workspace @x-fuji/protocol hardhat export-abi",
    "generate-types": "typechain --target ../../node_modules/@hovoh/typechain-ethers-multicall --out-dir src/types/contracts '../protocol/abis/**/*.json'",
    "analyze": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@ethersproject/abi": "^5.4.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.4.0",
    "@ethersproject/providers": "^5.6.8",
    "@ethersproject/units": "^5.7.0",
    "@hovoh/ethcall": "^9.9.8",
    "ethers": "^5.6.9",
    "tiny-invariant": "^1.2.0",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "@ethersproject/solidity": "^5.7.0",
    "@hovoh/typechain-ethers-multicall": "^10.1.3",
