{
  "name": "@x-fuji/sdk",
  "version": "0.1.0",
  "author": "Fujidao Labs OÃœ",
  "license": "MIT",
  "main": "dist/index",
  "typings": "dist/index",
  "files": [
    "dist",
    "src"
  ],
  "module": "dist/index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format cjs --dts",
    "start": "tsup src/index.ts --format cjs --dts --watch --onSuccess 'node dist/index.js'",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify --treeshake",
    "test": "jest --silent",
    "lint": "eslint . --fix",
    "prepare": "yarn build",
    "export-abis": "yarn workspace @x-fuji/protocol hardhat export-abi",
    "generate-types": "typechain --target ../../node_modules/@hovoh/typechain-ethers-multicall --out-dir src/types/contracts '../protocol/abis/**/*.json'"
  },
  "dependencies": {
    "@connext/sdk": "^0.2.1-rc.18",
    "@ethersproject/abi": "^5.4.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.0.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.4.0",
    "@ethersproject/providers": "^5.6.8",
    "@ethersproject/units": "^5.7.0",
    "@hovoh/ethcall": "^9.9.8",
    "axios": "1.1.3",
    "ethers": "^5.6.9",
    "tiny-invariant": "^1.2.0",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "@ethersproject/solidity": "^5.7.0",
    "@hovoh/typechain-ethers-multicall": "^10.1.3",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/chai": "^4.3.3",
    "@types/jest": "^29.2.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^2.0.0",
    "hardhat": "^2.12.0-esm.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  }
}
