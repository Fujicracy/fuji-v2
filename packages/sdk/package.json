    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "hardhat": "^2.12.0-esm.0",
    "size-limit": "^8.0.1",
    "ts-node": "^10.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
